# Environment Configuration Example
NODE_ENV=development
PORT=3000

# API Configuration
API_VERSION=1.0.0
API_RATE_LIMIT=100

# Security
API_KEYS=dev-key-12345,test-key-67890,admin-key-abcdef

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Error Handling
ENABLE_STACK_TRACES=true
ENABLE_ERROR_LOGGING=true